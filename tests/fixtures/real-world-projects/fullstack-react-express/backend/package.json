    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "express-validator": "^6.14.3",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.2",
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "typescript": "^4.9.5",
    "nodemon": "^2.0.20",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "supertest": "^6.3.3",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4"
